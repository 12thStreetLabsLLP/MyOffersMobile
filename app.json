{
  "expo": {
    "name": "My Offers",
    "slug": "my-offers",
    "version": "1.0.0",
    "description": "My offers app for showing awesome deals",
    "githubUrl": "https://github.com/12th/kittenTricks",
    "ios": {
      "bundleIdentifier": "com.name.myoffers",
      "appStoreUrl": "https://itunes.apple.com/us/app/kitten-tricks/id1246143230",
      "buildNumber": "3"
    },
    "android": {
      "package": "com.name.myoffers",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.12th.kittenTricks",
      "versionCode": 3,
      "googleServicesFile": "./google-services.json"
    },
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./src/assets/images/image-app-icon.png",
    "splash": {
      "image": "src/assets/images/image-splash.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "src/assets/images/*",
      "src/assets/fonts",
      "src/layouts/**/assets/*"
    ],
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#F7F9FC"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
